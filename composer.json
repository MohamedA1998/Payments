{
    "name": "mohameda1998/payments",
    "description": "A Laravel package for handling payments",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed",
            "email": "mohamed@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Payments\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Payments\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Payments\\PaymentsServiceProvider"
            ],
            "aliases": {
                "Payments": "Payments\\Facades\\Payments"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
